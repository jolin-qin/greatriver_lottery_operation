@import "icon.wxss";
@import "colorui.wxss";

page {
	background: #f7f8fd
}

.button-animation {
	animation-name: scale-down;
	animation-duration: 0.2s;
	animation-fill-mode: both;
	animation-timing-function: linear;

}

@keyframes scale-down {
	0% {
		opacity: 1;
		transform: scale(1)
	}

	30% {
		opacity: 1;
		transform: scale(0.8)
	}

	100% {
		opacity: 1;
		transform: scale(1)
	}
}

@font-face {
	font-family: "font22341";
	src: url(data:application/x-font-ttf;charset=utf-8;base64,);
	/* iOS 4.1- */
	font-style: normal;
	font-weight: normal;
}


.numfont {
	font-family: font22341;
}

.titlefont {
	font-weight: 700 !important;
}


.a-wobble {
	animation: wobble2 3s infinite linear;
	-webkit-animation: wobble2 3s infinite linear;
}

@keyframes wobble2 {
	0% {
		opacity: 1;
		-webkit-transform: scale(1);
	}

	25% {
		opacity: 1;
		-webkit-transform: scale(0.95);
	}

	50% {
		-webkit-transform: scale(1);
	}

	75% {
		-webkit-transform: scale(0.95);
	}

	100% {
		-webkit-transform: scale(1);
	}
}

.triangle-down-left {
	width: 20rpx;
	height: 20rpx;
	border-left: 10rpx solid transparent;
	border-right: 10rpx solid transparent;
	border-top: 15rpx solid #ff9700;
	position: absolute;
	left: 40rpx;
	bottom: -20rpx;
}

.triangle-down-right {
	width: 20rpx;
	height: 20rpx;
	border-left: 10rpx solid transparent;
	border-right: 10rpx solid transparent;
	border-top: 15rpx solid #ed1c24;
	position: absolute;
	right: 40rpx;
	bottom: -20rpx;
}
